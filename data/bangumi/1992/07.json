    "title": "サイレントメビウス2",
    "date": "1992-07-18",
    "bangumi": {
      "date": "1992-07-18",
      "platform": "剧场版",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/25/00/41879_k7k92.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/25/00/41879_k7k92.jpg",
        "large": "https://lain.bgm.tv/pic/cover/l/25/00/41879_k7k92.jpg",
