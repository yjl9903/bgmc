  {
    "title": "CAROL",
    "date": "1990-03-20T16:00:00.000Z",
    "bangumi": {
      "date": "1990-03-21",
      "platform": "剧场版",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/0f/f9/38312_8ZJE0.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/0f/f9/38312_8ZJE0.jpg",
