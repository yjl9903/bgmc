  {
    "title": "マクロス ゼロ",
    "date": "2002-12-21T16:00:00.000Z",
    "bangumi": {
      "date": "2002-12-21",
      "platform": "OVA",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/73/e5/3170_SfR27.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/73/e5/3170_SfR27.jpg",
