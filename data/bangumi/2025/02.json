    "title": "キミとアイドルプリキュア♪",
    "date": "2025-02-02",
    "bangumi": {
      "date": "2025-02-02",
      "platform": "TV",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/0a/40/526104_i2616.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/0a/40/526104_i2616.jpg",
