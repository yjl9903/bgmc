      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/97/19/2893_bSnRG.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/97/19/2893_bSnRG.jpg",
        "large": "https://lain.bgm.tv/pic/cover/l/97/19/2893_bSnRG.jpg",
