      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/11/e2/32489_sf8Sm.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/11/e2/32489_sf8Sm.jpg",
