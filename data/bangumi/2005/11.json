  {
    "title": "ガイキング LEGEND OF DAIKU-MARYU",
    "date": "2005-11-12",
    "bangumi": {
      "date": "2005-11-12",
      "platform": "TV",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/75/20/10145_I2Lo2.jpg",
