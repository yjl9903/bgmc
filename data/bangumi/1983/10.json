    "title": "特装機兵ドルバック",
    "date": "1983-10-07",
    "bangumi": {
      "date": "1983-10-07",
      "platform": "TV",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/53/0d/37233_5Cyjs.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/53/0d/37233_5Cyjs.jpg",
