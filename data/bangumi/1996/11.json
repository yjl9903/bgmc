  {
    "title": "マスターモスキートン",
    "date": "1996-11-21",
    "bangumi": {
      "date": "1996-11-21",
      "platform": "OVA",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/07/d2/7929_CJ7Nc.jpg",
