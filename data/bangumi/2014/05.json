    "title": "纳米神兵",
    "date": "2014-05-06",
    "bangumi": {
      "date": "2014-05-06",
      "platform": "TV",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/1d/2b/209662_58D4L.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/1d/2b/209662_58D4L.jpg",
        "large": "https://lain.bgm.tv/pic/cover/l/1d/2b/209662_58D4L.jpg",
