  {
    "title": "ビックリマン2000",
    "date": "1999-11-01",
    "bangumi": {
      "date": "1999-11-01",
      "platform": "TV",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/a3/74/339977_4WqZA.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/a3/74/339977_4WqZA.jpg",
