      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/d8/91/73928_2uL55.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/d8/91/73928_2uL55.jpg",
