  {
    "title": "ゲッターロボ",
    "date": "1974-04-03T16:00:00.000Z",
    "bangumi": {
      "date": "1974-04-04",
      "platform": "TV",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/76/c0/10405_H5VNQ.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/76/c0/10405_H5VNQ.jpg",
