    "bangumi": {
      "date": "2011-11-19",
      "platform": "剧场版",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/f4/d1/24592_c9c4X.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/f4/d1/24592_c9c4X.jpg",
