      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/2c/fd/20930_94WRd.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/2c/fd/20930_94WRd.jpg",
        "large": "https://lain.bgm.tv/pic/cover/l/2c/fd/20930_94WRd.jpg",
        "medium": "https://lain.bgm.tv/r/800/pic/cover/l/2c/fd/20930_94WRd.jpg",
