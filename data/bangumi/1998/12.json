  {
    "title": "新世紀GPXサイバーフォーミュラSIN",
    "bangumi": {
      "date": "1998-12-21",
      "platform": "OVA",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/d7/7f/1958_85NN3.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/d7/7f/1958_85NN3.jpg",
        "large": "https://lain.bgm.tv/pic/cover/l/d7/7f/1958_85NN3.jpg",
